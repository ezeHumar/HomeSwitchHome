<!DOCTYPE html>
<html>
  <head>
    <title>HomeSwitchHome</title>
    <nav class="navbar navbar-light bg-light">
        <%= link_to residences_path, class: 'navbar-brand' do %>
        <%= image_tag "HSH-Logo.svg", width: 50, height: 50, title: "Logo", class: "logo" %>
          HomeSwitchHome
        <% end %>

    </nav>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
