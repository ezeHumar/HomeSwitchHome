<% flash.each do |key, value| %>
<div class="alert alert-<%=key%>"><%=value%></div>
<%end%>
<h1>Residencias</h1>
<table class="table">
  <tr>
    <th>Nombre</th>
    <th>Ubicacion</th>
    <th>Capacidad</th>
    <th>Fotos</th>
  </tr>
  <%@residences.order(:name).each do |residence|%>
  <tr>
    <td><%= residence.name %></td>
    <td><%= residence.location %></td>
    <td><%= residence.capacity %></td>
    <td><%= residence.photos %></td>
  </tr>
 <%end%>
</table>
