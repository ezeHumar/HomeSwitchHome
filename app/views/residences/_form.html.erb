<%= simple_form_for @residence do |residence| %>
  <%= residence.input :name, label: 'Nombre', required: true %>
  <%= residence.input :location, label: 'Ubicacion', required: true %>
  <%= residence.input :capacity, label: 'Capacidad' %>
  <%= residence.input :photos ,label: 'URL'%>
  <%= residence.submit 'Guardar Residencia', class:'btn btn-info' %>
<% end %>
