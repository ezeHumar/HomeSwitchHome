<h3>Subastas</h3>
<table class="table">
  <tr>
    <th>Residencia</th>
    <th>Fecha</th>
    <th>Precio actual</th>
    <th>Mejor postor</th>
  </tr>
  <%@auctions.each do |auction|%>
  <tr>
    <td><%= auction.residence.name %></td>
    <td><%= auction.startDate %></td>
    <td><%= auction.amount %></td>
    <td><%= auction.email %></td>
  </tr>
 <%end%>
</table>
<div>
  <%= link_to 'Agregar subasta', new_admin_auction_path, class: 'btn btn-info'%>
</div>
