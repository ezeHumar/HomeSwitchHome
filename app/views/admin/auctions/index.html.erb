<br>
<% flash.each do |key, value| %>
<div class="alert alert-<%=key%>"><%=value%></div>
<%end%>
<h3>Subastas</h3>
<table class="table">
  <tr>
    <th>Id. De la residencia</th>
    <th>Fecha</th>
    <th>Precio actual</th>
    <th>Mejor postor</th>
  </tr>
  <%@auctions.each do |auction|%>
  <tr>
    <td><%= auction.residence_id %></td>
    <td><%= auction.startDate %></td>
    <td><%= auction.amount %></td>
    <td><%= auction.email %></td>
  </tr>
 <%end%>
</table>
<div>
  <%= link_to 'Agregar subasta', new_admin_auction_path, class: 'btn btn-info'%>
</div>
