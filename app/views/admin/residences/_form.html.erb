<%= simple_form_for [:admin, @residence] do |residence| %>
  <%= residence.input :name, required: true %>
  <%= residence.input :location, required: true %>
  <%= residence.input :capacity %>
  <%= residence.input :photos %>
  <div>
        <%= residence.submit class:'btn btn-info' %>
        <%= link_to 'Atras', admin_residences_path, class: 'btn btn-outline-info' %>
  </div>
<% end %>
